<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"

    android:background="#ffffff">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/name"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/i11"
        android:layout_toEndOf="@+id/i11"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="5dp"/>
    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/book1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/i11"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/title"
        android:textColor="#000000"
        android:layout_alignTop="@+id/i12"
        android:layout_toRightOf="@+id/i12"
        android:layout_toEndOf="@+id/i12"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="1dp"/>
    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginLeft="4dp"
        android:src="@drawable/time"
        android:id="@+id/i12"
        android:layout_below="@+id/i11"
        android:layout_marginTop="9dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/midsem"
        android:textColor="#000000"
        android:layout_above="@+id/i14"
        android:layout_toRightOf="@+id/i13"
        android:layout_toEndOf="@+id/i13"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="5dp"/>
    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/time"
        android:id="@+id/i13"
        android:layout_below="@+id/i12"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="9dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/endsem"
        android:textColor="#000000"
        android:layout_alignTop="@+id/i14"
        android:layout_toRightOf="@+id/i14"
        android:layout_toEndOf="@+id/i14"
        android:layout_marginLeft="7dp"
        />
    <ImageView
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/time"
        android:id="@+id/i14"
        android:layout_below="@+id/i13"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="9dp"/>
</RelativeLayout>
